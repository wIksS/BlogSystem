@model IEnumerable<BlogSystem.Web.ViewModels.NavigationElement>
<li class="widget row widget_categories">
        <h2 class="widgettitle">@ViewBag.Title</h2>
        <ul>
            @foreach (var element in Model)
            {
                <li class="cat-item">
                    <a href="/@ViewBag.User/@element.Title">@element.Title</a>
                </li>
            }
        </ul>
</li>

<body class="home blog mp6 highlander-enabled highlander-light infinite-scroll neverending">
    <div id="wrapper">
        <div id="page">
            <div class="menu-header">
                <ul class="menu">
                    <li class="menu-item current-menu-item"><a href="#">Home</a></li>
                    @Html.Action("ListPages", "Pages")
                </ul>
            </div>
            <div id="header" role="banner">
                <h1><a href="http://viktordakov.wordpress.com/">Viktor Dakov</a></h1>

                <div class="description">Programming blog</div>

                <div class="rss">
                    <a href="http://viktordakov.wordpress.com/feed/">Blog a lot</a>
                </div>
            </div>
            <div id="blog-container">
                <div id="sidebar" role="complementary">
                    <ul>
                        @Html.Action("ListCategories", "Categories")
                    </ul>
                </div>
                <div id="content" role="main">
                    <div id="content-inner" class="column">
                        @RenderBody()
                    </div><!-- #content-inner -->

                    <div id="nav-post">
                        <div class="navigation-bott">
                        </div>
                    </div>
                </div><div id="footer" role="contentinfo">

                </div><!-- #content -->
            </div>
        </div>
        <div id="footer-bott">
            <a href="https://wordpress.com/?ref=footer_blog" sl-processed="1">Blog at WordPress.com</a>. | <a href="https://wordpress.com/themes/greyzed/" title="Learn more about this theme" sl-processed="1">The Greyzed Theme</a>.
        </div>
    </div>

